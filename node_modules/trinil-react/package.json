{
  "name": "trinil-react",
  "version": "1.1.2",
  "description": "SVG icon library for React with locked stroke styles",
  "type": "module",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "peerDependencies": {
    "react": ">=16.8"
  },
  "devDependencies": {
    "@types/react": "^18.2.45",
    "typescript": "^5.3.3",
    "tsup": "^8.0.2"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts",
    "clean": "rm -rf dist && find src/icons -type f -name '*.tsx' -delete"
  }
}
